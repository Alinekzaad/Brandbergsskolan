@page
@model BrandbergFranvaro.Pages.Account.LogoutModel
@{
    ViewData["Title"] = "Utloggad";
}

<div class="text-center py-5">
    <i class="bi bi-check-circle text-success" style="font-size: 4rem;"></i>
    <h2 class="mt-3">Du har loggats ut</h2>
    <p class="text-muted">Tack för besöket!</p>
    <a asp-page="/Account/Login" class="btn btn-primary">
        <i class="bi bi-box-arrow-in-right me-2"></i>Logga in igen
    </a>
</div>

